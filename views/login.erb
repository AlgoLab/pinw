<head>
  <title>PinW | Login</title>

  <%# JS & CSS #%>
  <%= erb :'includes/header' %>
</head>
<body>




  <%# PAGE CODE HERE #%>

  <div class="container">
    <form role="form" method="post">
      <div class="form-group">
        <label for="InputUser">Nickname</label>
        <input type="text" class="form-control" id="InputUser" placeholder="Enter nickname" name="InputUser">
      </div>
      <div class="form-group">
        <label for="InputPassword">Password</label>
        <input type="password" class="form-control" id="InputPassword" placeholder="Password" name="InputPassword">
      </div>
      <button type="submit" class="btn btn-default">Login</button>
    </form>
  </div>

</body>